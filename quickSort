#include <iostream>
#include <algorithm>
using namespace std;

void print(int a[], int n) {
    for (int i = 0; i < n; i++) {
        cout << a[i] << " ";
    }
    cout << endl;
}

void Partition(int a[], int l, int r, int n) {
    if (l >= r) return;
    int x = a[l];
    int i = l + 1, j = r;
    while (i <= j) {
        while (a[i] <= x && i <= j) i++;
        while (a[j] > x && i <= j) j--;
        if (i < j) swap(a[i], a[j]);
    }
    swap(a[l], a[j])
    print(a, n);
    Partition(a, l, j - 1, n);
    Partition(a, j + 1, r, n);
}

void QuickSort(int a[], int n) {
    Partition(a, 0, n - 1, n);
}

int main() {
    int a[100], n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    QuickSort(a, n);
    return 0;
}

